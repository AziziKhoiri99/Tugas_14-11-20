                                    <table class="table table-bordered" id="dataTable" width="100%" 
                                           cellspacing="0">
                                        <thead>
                                            <tr>
                                                <th>No.KK</th>
                                                <th>No.KTP</th>
                                                <th>Nama Kepala Keluarga</th>
                                                <th>Alamat Rumah</th>
                                                <th>Daerah</th>
                                                <th>Jumlah Bantuan</th>
                                                <th>Opsi</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {{#each data}}
                                            <tr>
                                                <td>{{ NO_KK }}</td>
                                                <td>{{ NO_KTP }}</td>
                                                <td>{{ NAMA_KEPALA }}</td>
                                                <td>{{ ALAMAT }}</td>
                                                <td>{{ DAERAH }}</td>
                                                <td>{{ JUMLAH_BANTUAN }}</td>
                                                <td><a href="/hapus-barang/{{ NO_KK }}">
                                                <button class="badge badge-danger">Batal</button></a></td>
                                             </tr>
                                        {{/each}}
                                        </tbody>
                                    </table>
                                </div>
