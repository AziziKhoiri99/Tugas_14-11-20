            <div class="py-5" id="layoutSidenav_content">
                <main>
                    <div class="container-fluid">
                        <h1 class="mt-4 font-weight-bold">Data Bantuan UMKM Tahun 2020</h1>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item active">Keep Strong, Never Give Up</li>
                        </ol>
                        <div class="card mb-4">
                            <div class="card-header">
                                <i class="fas fa-table mr-1"></i>
                                Data Input Warga Yang Menerima Bantuan UMKM
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                <div class="col-sm-12 col-md-6">
                                                <div id="dataTable_filter" class="dataTables_filter">
                                                    <p>Search:<input type="search" 
                                                     class="form-control form-control-sm" 
                                                     placeholder="" aria-controls="dataTable">
                                                     </p>
                                                     <!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
  Daftar Disini
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Biodata Siswa</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
            <form action="/data" method="POST">
    <input type="text" name="inputnokk" class="form-control form-control-sm" aria-controls="dataTable" placeholder="No. Kartu Keluarga">
    <input type="text" name="inputnoktp" class="form-control form-control-sm" aria-controls="dataTable" placeholder="No. KTP">
    <input type="text" name="inputnama" class="form-control form-control-sm" aria-controls="dataTable" placeholder="Nama Kepala Keluarga">
    <input type="text" name="inputalamat" class="form-control form-control-sm" aria-controls="dataTable" placeholder="Alamat Rumah">
    <input type="text" name="inputdaerah" class="form-control form-control-sm" aria-controls="dataTable" placeholder="Daerah">
    <input type="text" name="inputjumlah" class="form-control form-control-sm" aria-controls="dataTable" placeholder="Jumlah Bantuan">
    <br>
    <button type="button" class="btn btn-primary" data-dismiss="modal">Submit</button>
   </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
